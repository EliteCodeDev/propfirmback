  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:withdrawals": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/withdrawals.seed.ts",
    "seed:users": "ts-node -r tsconfig-paths/register src/seeds/seed-users.ts",
    "seed:challenges": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-challenges.ts",
    "seed:broker-accounts": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-broker-accounts.ts",
    "seed:challenge-templates": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-challenge-templates.ts",
    "seed:challenges:real": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-challenges-real.ts",
    "seed:broker-accounts:real": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-broker-accounts-real.ts",
    "seed:db-sync": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/db-sync.ts",
    "seed:user:specific": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-specific-user.ts",
    "seed:certificates": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-certificates.ts",
    "seed:orders": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-orders.ts",
    "seed:verifications": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-verifications.ts",
    "seed:specific-challenge": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-specific-challenge.ts",
    "seed:styles": "ts-node -r tsconfig-paths/register -r reflect-metadata src/seeds/seed-styles.ts"
  },
