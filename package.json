    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "handlebars": "^4.7.8",
    "joi": "^17.13.3",
    "minio": "^8.0.5",
    "moment": "^2.30.1",
    "nest-winston": "^1.10.2",
    "nestjs-minio": "^2.6.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
